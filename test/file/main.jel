import(../../std/clet/wrapper.jel)

# initialize
call(:clet_init)

# make variable
call(:clet_set "qwe asd" "asd")
call(:clet_set deneme "naber dostum!")
call(:clet_set a-number 20)

# get variable
print(call(:clet_get "qwe"))
print(call(:clet_get deneme))
print(bigger(call(:clet_get a-number) 10))

# free
call(:clet_free)